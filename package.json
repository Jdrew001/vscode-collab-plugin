{
  "name": "vscode-collab-plugin",
  "displayName": "vscode-collab-plugin",
  "publisher": "Pascal-D",
  "description": "TBA",
  "version": "0.0.1",
  "repository": "https://github.com/sekassel-research/vscode-collab-plugin",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-collab-plugin.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-collab",
          "title": "VS-Code Collab",
          "icon": "media/HandshakeDavid.svg"
        }
      ]
    },
    "views": {
      "vscode-collab": [
        {
          "id": "vscode-collab-activeUsers",
          "name": "Active Users"
        },
        {
        "type": "webview",
        "id": "vscode-collab-chatView",
        "name": "Chat"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "ws": "^8.11.0"
  }
}
